# external address used by n8n for callbacks/webhooks
N8N_HOST=localhost
N8N_PROTOCOL=http
N8N_PORT=5678
WEBHOOK_URL=http://localhost:5678/

# recommended basics
GENERIC_TIMEZONE=Etc/UTC
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=change-me
# set a strong 32+ char key for credential encryption
N8N_ENCRYPTION_KEY=please-change-me-to-a-strong-secret
